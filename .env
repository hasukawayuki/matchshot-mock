# Replicate API Token (https://replicate.com/)
REACT_APP_REPLICATE_API_TOKEN=your_replicate_token_here

# FaceSwap API URL (カスタムサーバーの場合)
REACT_APP_FACESWAP_API_URL=http://localhost:5000

# モックモード（true にするとAPI呼び出しせずにモック画像を使用）
REACT_APP_MOCK_MODE=true

# Amplify 設定
PUBLIC_URL=.
GENERATE_SOURCEMAP=false
